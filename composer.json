{
    "name": "f4php/framework",
    "description": "HookManager is internal hook management SDK for F4, a lightweight web application framework",
    "keywords": ["f4","framework"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "F4\\": "src/F4"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "F4\\": "tests/F4"
        }
    },
    "authors": [
        {
            "name": "Dennis Kreminsky",
            "email": "dennis@kreminsky.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "nodejs-php-fallback/nodejs-php-fallback": true
        }
    },
    "scripts": {
        "test": "phpunit --display-warnings --display-phpunit-deprecations --fail-on-deprecation tests",
        "phpstan": "phpstan --level=1 analyse src tests"
    },
    "require": {
        "php": "^8.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    }
}
